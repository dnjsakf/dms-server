{
  "name": "dms-server",
  "version": "0.1.0",
  "private": true,
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "scripts": {
    "test": "cross-env NODE_ENV=local jest --watchAll=false --detectOpenHandles ./tests/cache.test.js",
    "local": "cross-env NODE_ENV=local nodemon --exec babel-node src/app.js",
    "dev": "cross-env NODE_ENV=development babel-node src/app.js",
    "start": "cross-env NODE_ENV=production babel-node src/app.js",
    "crypto": "cross-env NODE_ENV=development babel-node ./src/utils/cryptoUtil.js",
    "migrate": "cross-env NODE_ENV=development babel-node ../node_modules/sequelize-cli/lib/sequelize db:migrate --config ./migration/config/config.js --migrations-path ./migration/migrations --models-path ./src/models",
    "migrate:reset": "cross-env NODE_ENV=development babel-node ../node_modules/sequelize-cli/lib/sequelize db:migrate:undo:all --config ./migration/config/config.js --migrations-path ./migration/migrations --models-path ./src/models",
    "docker:build": "docker build -t dnjsakf/dms-server:latest -f ./cicd/local/Dockerfile .",
    "docker:run": "docker run -d --add-host host.docker.internal:host-gateway --network bridge --name dms-server dnjsakf/dms-server:latest"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/node": "^7.25.0",
    "@babel/preset-env": "^7.25.3",
    "@babel/register": "^7.24.6",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.4",
    "sequelize-cli": "^6.6.2"
  },
  "dependencies": {
    "connect-redis": "^7.1.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "graphql": "^15.9.0",
    "graphql-http": "^1.22.1",
    "jsonwebtoken": "^9.0.2",
    "mariadb": "^2.5.6",
    "pg": "^8.12.0",
    "redis": "^4.7.0",
    "ruru": "^2.0.0-beta.13",
    "sequelize": "^6.37.3",
    "sequelize-hierarchy": "^2.0.4"
  }
}
